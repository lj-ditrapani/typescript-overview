{
  "name": "todo-cli",
  "version": "0.0.1",
  "description": "todo cli",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r ts-node/register src/index.ts",
    "test": "nyc -e .ts mocha -r ts-node/register \"test/**/*_test.{js,ts}\"",
    "test1": "mocha -r ts-node/register",
    "fmt": "prettier --parser typescript --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tlint": "tslint --fix -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "npm run tlint && npm run fmt",
    "tsc": "tsc",
    "doc": "typedoc --out doc/ src",
    "all": "npm run lint && npm test"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 94,
    "statements": 94,
    "functions": 94,
    "branches": 76,
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/output.ts",
      "**/*.d.ts",
      "test/**"
    ]
  },
  "keywords": [
    "todo"
  ],
  "author": "Jonathan Di Trapani",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^10.12.18",
    "chalk": "^2.4.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^7.0.3",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "sinon": "^7.2.2",
    "source-map-support": "^0.5.9",
    "ts-mockito": "^2.3.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typedoc": "^0.14.0"
  }
}
