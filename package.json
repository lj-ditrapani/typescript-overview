{
  "name": "typescript-overview",
  "version": "0.0.1",
  "description": "TypeScript Overivew",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r ts-node/register src/index.ts",
    "test": "nyc -e .ts mocha -r ts-node/register \"test/**/*_test.ts\"",
    "test1": "mocha -r ts-node/register",
    "fmt": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"*.json\"",
    "tlint": "tslint --fix -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "npm run tlint && npm run fmt",
    "tsc": "tsc",
    "doc": "typedoc --out doc/ src",
    "all": "npm run lint && tsc && npm test"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/result.ts",
      "**/*.d.ts",
      "test/**"
    ]
  },
  "keywords": [
    "overview",
    "todo"
  ],
  "author": "Jonathan Di Trapani",
  "repository": "https://github.com/lj-ditrapani/typescript-overview",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^14.14.41",
    "chalk": "^4.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.20.35"
  }
}
