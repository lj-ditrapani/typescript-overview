{
  "name": "typescript-overview",
  "version": "0.0.1",
  "description": "TypeScript Overivew",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r ts-node/register src/index.ts",
    "test": "jest --coverage",
    "lint": "eslint --fix *.js *.json '*/**/*.ts'",
    "ncu": "ncu -u && rm -fr node_modules package-lock.json && npm install",
    "tsc": "tsc",
    "doc": "typedoc --out doc/ src",
    "all": "npm run lint && tsc && npm test"
  },
  "keywords": [
    "overview",
    "todo"
  ],
  "author": "Jonathan Di Trapani",
  "repository": "https://github.com/lj-ditrapani/typescript-overview",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^18.0.5",
    "chalk": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "npm-check-updates": "^15.3.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.6",
    "typedoc": "^0.23.7"
  }
}
