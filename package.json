{
  "name": "typescript-overview",
  "version": "0.0.1",
  "description": "TypeScript Overivew",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r ts-node/register src/index.ts",
    "test": "nyc -e .ts mocha -r ts-node/register \"test/**/*_test.{js,ts}\"",
    "test1": "mocha -r ts-node/register",
    "fmt": "prettier --parser typescript --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "fmtjs": "prettier --write \"src/**/*.js\" \"test/**/*.js\" \"src/**/*.json\" \"test/**/*.json\" \"*.json\"",
    "tlint": "tslint --fix -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "npm run tlint && npm run fmt && npm run fmtjs",
    "tsc": "tsc",
    "doc": "typedoc --out doc/ src",
    "all": "npm run lint && tsc && npm test"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/output.ts",
      "**/*.d.ts",
      "test/**"
    ]
  },
  "keywords": [
    "todo"
  ],
  "author": "Jonathan Di Trapani",
  "repository": "https://github.com/lj-ditrapani/typescript-overview",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^12.0.2",
    "chalk": "^2.4.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "source-map-support": "^0.5.12",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2"
  }
}
